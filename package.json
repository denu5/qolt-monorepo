{
    "name": "qolt-monorepo",
    "version": "0.0.1",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo build",
        "dev": "turbo dev",
        "tsc": "turbo tsc",
        "test": "turbo test",
        "lint-fix": "turbo lint-fix",
        "format-fix": "prettier --config .prettierrc --write --ignore-unknown -l .",
        "clean": "npm exec --workspaces -- shx rm -rf .next .turbo .contentlayer dist compiled build-next-static",
        "clean-reset": "npm run clean && npm exec --workspaces -- shx rm -rf node_modules && shx rm -rf node_modules",
        "check-updates": "npm exec --workspaces -- npx npm-check-updates -u && npx npm-check-updates -u",
        "turbo": "turbo"
    },
    "dependencies": {
        "@emotion/react": "11.11.4",
        "@emotion/styled": "11.11.5",
        "@mui/icons-material": "5.15.19",
        "@mui/joy": "^5.0.0-beta.36",
        "@mui/material": "^5.15.19",
        "@mui/material-nextjs": "5.15.11",
        "@next/third-parties": "14.2.3",
        "@vercel/analytics": "1.3.1",
        "@vercel/speed-insights": "1.0.11",
        "contentlayer": "0.3.4",
        "date-fns": "3.6.0",
        "nanoid": "^5.0.7",
        "next": "^14.2.3",
        "next-contentlayer": "0.3.4",
        "nprogress": "0.2.0",
        "react": "18.3.1",
        "react-dom": "18.3.1"
    },
    "devDependencies": {
        "@types/node": "20.13.0",
        "@types/nprogress": "0.2.3",
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
        "prettier": "3.2.5",
        "shx": "0.3.4",
        "ts-node": "10.9.2",
        "tsup": "8.0.2",
        "turbo": "1.13.3",
        "typescript": "5.4.5"
    },
    "overrides": {
        "next-contentlayer": {
            "next": "$next"
        }
    }
}
