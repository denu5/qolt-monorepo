{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", "build-next-static/**"]
        },
        "lint-fix": {
            "cache": false,
            "persistent": true
        },
        "tsc": {
            "dependsOn": ["build"]
        },
        "test": {
            "dependsOn": ["build"]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "check-updates": {
            "cache": false
        }
    }
}
