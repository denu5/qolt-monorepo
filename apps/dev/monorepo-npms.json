{
    "root": {
        "name": "qolt-monorepo",
        "version": "0.0.1",
        "workspaces": ["apps/*", "packages/*"],
        "scripts": {
            "build": "turbo build",
            "dev": "turbo dev",
            "tsc": "turbo tsc",
            "test": "turbo test",
            "lint-fix": "turbo lint-fix",
            "format-fix": "prettier --config .prettierrc --write --ignore-unknown -l .",
            "clean": "npm exec --workspaces -- shx rm -rf .next .turbo .contentlayer dist compiled build-next-static",
            "clean-reset": "npm run clean && npm exec --workspaces -- shx rm -rf node_modules && shx rm -rf node_modules",
            "check-updates": "npm exec --workspaces -- npx npm-check-updates -u && npx npm-check-updates -u",
            "turbo": "turbo",
            "postinstall": "node dump-monorepo-info.js /apps/dev/monorepo-npms.json"
        },
        "dependencies": {
            "@emotion/react": "11.13.0",
            "@emotion/styled": "11.13.0",
            "@mui/icons-material": "5.16.6",
            "@mui/joy": "^5.0.0-beta.48",
            "@mui/material": "^5.16.6",
            "@mui/material-nextjs": "5.16.6",
            "@next/third-parties": "14.2.5",
            "@vercel/analytics": "1.3.1",
            "@vercel/speed-insights": "1.0.12",
            "contentlayer": "0.3.4",
            "date-fns": "3.6.0",
            "nanoid": "^5.0.7",
            "next": "^14.2.5",
            "next-contentlayer": "0.3.4",
            "nprogress": "0.2.0",
            "react": "18.3.1",
            "react-dom": "18.3.1"
        },
        "devDependencies": {
            "@types/node": "22.1.0",
            "@types/nprogress": "0.2.3",
            "@types/react": "18.3.3",
            "@types/react-dom": "18.3.0",
            "eslint": "^9.8.0",
            "prettier": "3.3.3",
            "shx": "0.3.4",
            "ts-node": "10.9.2",
            "tsup": "8.2.4",
            "turbo": "2.0.11",
            "typescript": "5.5.4"
        },
        "overrides": {
            "next-contentlayer": {
                "next": "$next"
            }
        },
        "packageManager": "npm@10.2.4"
    },
    "workspaces": {
        "apps": [
            {
                "name": "dev",
                "description": "üë∑ Dev-Playground ‚Ä¢ Experiments using @mui/joy, next.js app-router stuff, React RSC... you name it.",
                "version": "0.0.1",
                "scripts": {
                    "build": "next build",
                    "dev": "next dev --port 3000",
                    "lint": "eslint --max-warnings 0 .",
                    "lint-fix": "eslint --fix ."
                },
                "dependencies": {
                    "jsdom": "^24.1.1",
                    "prism-react-renderer": "^2.3.1"
                },
                "devDependencies": {
                    "config-tsconfig": "*"
                }
            },
            {
                "name": "github-lists",
                "description": "‚≠ê GitHub List Bookmarks ‚Ä¢ Manage and display lists of GitHub repos using MDX files and GitHub Stars/Lists for repo data with the GitHub API.",
                "version": "0.0.1",
                "scripts": {
                    "build": "next build",
                    "dev": "next dev --port 3002",
                    "lint": "eslint --max-warnings 0 .",
                    "lint-fix": "eslint --fix ."
                },
                "dependencies": {
                    "@iarna/toml": "^2.2.5",
                    "@qolt/app-components": "*",
                    "@qolt/app-contentlayer": "*",
                    "@qolt/data-github": "*",
                    "mdx-mermaid": "^2.0.0",
                    "mermaid": "^10.9.1",
                    "mongodb": "^6.8.0",
                    "packageurl-js": "^1.2.1"
                },
                "devDependencies": {
                    "config-tsconfig": "*"
                }
            },
            {
                "name": "insta-lr",
                "version": "0.0.1",
                "description": "üì∑ Instagram Clone x Adobe Lightroom Albums ‚Ä¢ Manage and display photo albums via MDX files and Adode Lightroom Album-Sharing as API",
                "scripts": {
                    "build": "next build",
                    "dev": "next dev --port 3001",
                    "lint": "eslint --max-warnings 0 .",
                    "lint-fix": "eslint --fix ."
                },
                "dependencies": {
                    "@googlemaps/markerclusterer": "^2.5.3",
                    "@qolt/app-components": "*",
                    "@qolt/app-contentlayer": "*",
                    "@qolt/data-lightroom": "*",
                    "@vis.gl/react-google-maps": "^1.1.0",
                    "geolib": "^3.3.4",
                    "jsdom": "^24.1.1"
                },
                "devDependencies": {
                    "@types/google.maps": "^3.55.12",
                    "@types/jsdom": "^21.1.7",
                    "config-tsconfig": "*"
                }
            }
        ],
        "packages": [
            {
                "name": "@qolt/app-components",
                "version": "0.0.1",
                "type": "module",
                "main": "dist/index.cjs",
                "module": "dist/index.js",
                "types": "dist/index.d.ts",
                "exports": {
                    ".": {
                        "require": "./dist/index.cjs",
                        "import": "./dist/index.js",
                        "types": "./dist/index.d.ts"
                    },
                    "./_client": {
                        "require": "./dist/_client/index.cjs",
                        "import": "./dist/_client/index.js",
                        "types": "./dist/_client/index.d.ts"
                    }
                },
                "scripts": {
                    "build": "tsup",
                    "dev": "npm run build -- --watch",
                    "lint": "eslint --max-warnings 0 .",
                    "lint-fix": "eslint --fix .",
                    "tsc": "tsc -p tsconfig.json"
                },
                "devDependencies": {
                    "config-tsconfig": "*"
                },
                "dependencies": {
                    "react-resizable-panels": "^2.0.22"
                }
            },
            {
                "name": "@qolt/app-contentlayer",
                "version": "0.0.1",
                "main": "./src/index.ts",
                "scripts": {
                    "lint": "eslint --max-warnings 0 .",
                    "lint-fix": "eslint --fix ."
                },
                "devDependencies": {
                    "config-tsconfig": "*"
                }
            },
            {
                "name": "eslint-config-base",
                "version": "0.0.1",
                "main": "index.js",
                "devDependencies": {
                    "@typescript-eslint/eslint-plugin": "8.0.0",
                    "@typescript-eslint/parser": "8.0.0",
                    "eslint": "9.8.0",
                    "eslint-config-prettier": "9.1.0",
                    "eslint-import-resolver-typescript": "3.6.1",
                    "eslint-plugin-import": "2.29.1",
                    "eslint-plugin-jest": "28.7.0",
                    "eslint-plugin-jest-dom": "5.4.0",
                    "eslint-plugin-prettier": "5.2.1",
                    "eslint-plugin-react": "7.35.0",
                    "eslint-plugin-react-hooks": "4.6.2",
                    "eslint-plugin-testing-library": "6.2.2"
                }
            },
            {
                "name": "config-tsconfig",
                "version": "0.0.1"
            },
            {
                "name": "@qolt/data-github",
                "version": "0.0.1",
                "main": "./dist/index.js",
                "files": ["dist", "README.md"],
                "scripts": {
                    "build": "tsup",
                    "dev": "npm run build -- --watch",
                    "lint": "eslint --max-warnings 0 .",
                    "lint-fix": "eslint --fix .",
                    "tsc": "tsc -p tsconfig.json"
                },
                "dependencies": {
                    "cheerio": "1.0.0-rc.12",
                    "packageurl-js": "^1.2.1"
                },
                "devDependencies": {
                    "config-tsconfig": "*"
                }
            },
            {
                "name": "@qolt/data-lightroom",
                "version": "0.0.1",
                "main": "./dist/index.js",
                "files": ["dist", "README.md"],
                "scripts": {
                    "build": "tsup",
                    "dev": "npm run build -- --watch",
                    "lint": "eslint --max-warnings 0 .",
                    "lint-fix": "eslint --fix .",
                    "tsc": "tsc -p tsconfig.json",
                    "test": "jest --verbose",
                    "test-watch": "jest --verbose --watch",
                    "test-coverage": "jest --coverage --verbose"
                },
                "devDependencies": {
                    "config-tsconfig": "*",
                    "@types/node": "latest"
                }
            }
        ]
    }
}
